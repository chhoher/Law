<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- add By Jia 2017-07-18 文管新增法院文的債權文件相對人 -->
<hibernate-mapping>
    <class name="com.myjs.doc.documents.model.LDocCourtDocDebtDocRela" table="L_DOC_COURT_DOC_DEBT_DOC_RELA" schema="dbo" catalog="Legal" optimistic-lock="version">
        <id name="courtDocDebtDocRelaId" type="string">
            <column name="court_doc_debt_doc_rela_id"/>
            <generator class="uuid" />
        </id>
		<property name="ID" type="string">
			<column name="ID" length="10" />
		</property>
		<property name="name" type="string">
			<column name="name" length="15"/>
		</property>
		<property name="courtDocId" type="int">
			<column name="court_doc_id" />
		</property>
	</class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- add By Jia 2017-07-13 文管新增執行名義的其他相對人 -->
<hibernate-mapping>
    <class name="com.myjs.doc.documents.model.LDocCentitlementOtherRela" table="L_DOC_CENTITLEMENT_OTHER_RELA" schema="dbo" catalog="Legal" optimistic-lock="version">
        <id name="centitlementOtherRelaId" type="string">
            <column name="centitlement_other_rela_id"/>
            <generator class="uuid" />
        </id>
		<property name="ID" type="string">
			<column name="ID" length="10" />
		</property>
		<property name="name" type="string">
			<column name="name" length="15"/>
		</property>
		<property name="centitlementId" type="int">
			<column name="centitlement_id" />
		</property>
	</class>
</hibernate-mapping>
